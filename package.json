{
  "name": "solana-trading-bot-server",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "DOTENV_CONFIG_PATH=./.env node -r dotenv/config dist/index.js",
